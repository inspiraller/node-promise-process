{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-transform-typescript",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    "@babel/plugin-transform-runtime",
    ["module-resolver", {
      "extensions": [".js", ".ts"],
      "root": ["./"],
      "alias": {
        "__tests__": "./__tests__",
        "src": "./src",
        "bin": "./bin"
      }
    }]
  ]
}
